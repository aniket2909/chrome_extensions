{
    "name":"Youtube_thumbnail_shuffler",
    "description":"Shuffles Youtube Thumbnails",
    "version":"1.0",
    "manifest_version":3,
    "permissions":[
        "activeTab",
        "scripting"
    ],
    "content_scripts":[
        {
            "matches":["*://*.youtube.com/*"],
            "js":["thumbnail_shuffler_main.js"],
            "run_at":"document_idle"
        }
    ]
}